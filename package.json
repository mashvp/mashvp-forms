{
  "name": "mashvp-forms",
  "version": "0.1.0-alpha.0",
  "description": "No-bullsh!t form plugin",
  "main": "js/index.js",
  "author": "Mashvp",
  "license": "MIT",
  "scripts": {
    "sass": "node-sass styles/index.scss assets/style.css --include-path ./node_modules --output-style compressed",
    "watch-sass": "node-sass-chokidar -w styles/index.scss assets/style.css --include-path ./node_modules --output-style nested --source-map true",
    "js": "rm -f js/chunks/* && webpack --mode production",
    "watch-js": "rm -f js/chunks/* && webpack --watch --mode development",
    "watch": "concurrently 'npm:watch-sass' 'npm:watch-js'",
    "build": "concurrently 'npm:sass' 'npm:js'",
    "tr": "po2json languages/mashvp-forms-fr_FR.po languages/mashvp-forms-fr_FR-mashvp-forms--admin-script.json -d mashvp-forms -f jed1.x"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "node-sass": "^5.0.0",
    "node-sass-chokidar": "^1.5.0",
    "po2json": "^0.4.5",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "pubsub-js": "^1.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-smooth-dnd": "^0.11.1",
    "stimulus": "^1.1.1",
    "uuid": "^8.3.1"
  }
}
